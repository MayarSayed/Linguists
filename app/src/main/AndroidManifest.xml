    package="com.example.go_workingspace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BookForm"></activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".CwsProfileActivity" />
        <activity android:name=".SignUp_" />
        <activity android:name=".ListActivity" />
        <activity android:name=".Userprofile" ></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".Data.Provider"
            android:authorities="com.example.android.go_working_space"
            android:exported="false" />
    </application>

</manifest>