<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.go_workingspace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CwsNewActivity"></activity>
        <activity
            android:name=".BookForm"
            android:label="Book Form" />
        <activity
            android:name=".SignInActivity"
            android:label="Sign in" />
        <activity
            android:name=".CwsProfileActivity"
            android:label="Co-Working Space" />
        <activity
            android:name=".SignUp_"
            android:label="Sign Up" />
        <activity
            android:name=".ListActivity"
            android:label="Co-Working spaces" />
        <activity
            android:name=".Userprofile"
            android:label="Profile" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".Data.Provider"
            android:authorities="com.example.android.go_working_space"
            android:exported="false" />
    </application>

</manifest>